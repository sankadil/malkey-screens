				var lsRegNo:String = txtRegNo.text.toUpperCase();
				var lsVehiModelID:String = crVehicleModel.lastResult[cboModel.selectedIndex].id.vehimodelid;
				
				oUpdatedVehicleRec = new Com_dspl_malkey_domain_Fvehicle();
				
				// General page tab
